    "@types/node": "^9.4.6",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "^4.3.1",
    "browserify": "^16.2.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "del-cli": "1.1.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0",
    "source-map-support": "^0.5.4",
    "ts-node": "5.0.1",
