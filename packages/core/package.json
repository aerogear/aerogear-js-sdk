{
  "name": "@aerogearservices/core",
  "version": "1.0.0",
  "description": "Core configuration parser for AeroGear services",
  "types": "src/index.ts",
  "author": "Aerogear <aerogear@googlegroups.com>",
  "license": "Apache-2.0",
  "main": "src/",
  "scripts": {
    "clean": "del coverage_report src/**/*.js src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "test": "nyc mocha"
  },
  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "coverage_report",
    "lines": 75,
    "functions": 100,
    "branches": 80
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/chai-as-promised": "7.1.0",
    "@types/cordova-plugin-camera": "2.4.0",
    "@types/mocha": "2.2.48",
    "@types/node": "^9.4.6",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "4.1.3",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "del-cli": "1.1.0",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "sinon": "4.3.0",
    "source-map-support": "0.5.3",
    "ts-node": "4.1.0",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "@types/lodash": "4.14.104",
    "lodash": "4.17.5"
  }
}
