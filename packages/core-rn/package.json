{
  "name": "@aerogear/core-rn",
  "version": "0.0.1",
  "description": "JavaScript Core SDK for AeroGear services",
  "main": "www/index.js",
  "author": "Aerogear <aerogear@googlegroups.com>",
  "keywords": [
    "aerogear",
    "mobile"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "clean": "del coverage .nyc_output dist types",
    "build": "tsc && browserify dist/index.js --standalone 'aerogear.core' -o www/bundle.js",
    "watch": "tsc --watch",
    "test": "nyc mocha"
  },
  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "lines": 75,
    "functions": 100,
    "branches": 80
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/chai": "4.1.2",
    "@types/chai-as-promised": "7.1.0",
    "@types/lodash.find": "^4.6.3",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.4.6",
    "@types/proxyquire": "1.3.28",
    "@types/react-native": "^0.55.4",
    "@types/sinon": "^4.3.1",
    "@types/uuid": "^3.4.3",
    "@aerogear/core": "0.0.1",
    "browserify": "^16.2.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "del-cli": "1.1.0",
    "mocha": "^5.0.5",
    "mockttp": "^0.7.1",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0",
    "source-map-support": "^0.5.4",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bluebird": "^3.5.1",
    "lodash.find": "^4.6.0",
    "react-native": "^0.55.3",
    "uuid": "^3.2.1"
  }
}
